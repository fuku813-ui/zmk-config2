#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

 / {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &kp Q   &kp W   &kp E   &kp R   &kp T
                &kp A   &kp S   &kp D   &kp F   &kp G
                &kp Z   &kp X   &kp C   &kp V   &kp B
                &kp ESC &kp TAB &kp SPC &kp ENT &kp BSPC
            >;
        };
    };
};

