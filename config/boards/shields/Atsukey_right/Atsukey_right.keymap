#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &kp Y   &kp U   &kp I   &kp O   &kp P
                &kp H   &kp J   &kp K   &kp L   &kp APOS
                &kp N   &kp M   &kp COMMA &kp DOT &kp SLASH
                &kp BSPC &kp ENT &kp SPC  &kp TAB  &kp ESC
            >;
        };
    };
};
